#include<stdio.h> 
int main()
{
  	int i, n;
 
  	printf("Please Enter the Maximum Limit Value \n  :  ");
  	scanf("%d", &n);
  
  	printf("\n Odd Numbers between 1 and %d are : \n", n);
  	for(i = 1; i <= n; i++)
  	{
    	if ( i % 2 != 0 ) 
    	{
  			printf(" %d\t", i);
    	}
  	}
 
  	return 0;
}
